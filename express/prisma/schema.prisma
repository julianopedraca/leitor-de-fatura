generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//  "contribution_public_ilum": "35,92"}

model ConsumoEnergia {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  client_number Int 
  refer_to String @db.VarChar(10)
  eletric_energy_quantity Int
  eletric_energy_amount  Decimal @db.Decimal(9,8)
  energy_SCEE_without_ICMS Int
  energy_compensated_GD_I_quantity Int
  energy_compensated_GD_I_value Decimal @db.Decimal(9,8)
  contribution_public_ilum Decimal @db.Money
}
